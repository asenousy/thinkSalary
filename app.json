{
  "expo": {
    "name": "thinkSalary",
    "slug": "think-salary",
    "version": "1.1.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e90ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "{{AD_UNIT_ID_IOS}}"
      },
      "appStoreUrl": "https://apps.apple.com/us/app/thinksalary/id1556930096",
      "bundleIdentifier": "com.asenousy.think-salary"
    },
    "android": {
      "permissions" : [],
      "icon": "./assets/icon-android.png",
      "softwareKeyboardLayoutMode": "pan",
      "config": {
        "googleMobileAdsAppId": "{{AD_UNIT_ID_ANDROID}}"
      },
      "package": "com.asenousy.thinksalary",
      "versionCode": 2,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.asenousy.thinksalary"
    }
  }
}
