{
  "expo": {
    "name": "thinkSalary",
    "slug": "think-salary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e90ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "{{AD_UNIT_ID_IOS}}"
      },
      "bundleIdentifier": "com.asenousy.think-salary"
    },
    "android": {
      "icon": "./assets/icon-android.png",
      "softwareKeyboardLayoutMode": "pan",
      "config": {
        "googleMobileAdsAppId": "{{AD_UNIT_ID_ANDROID}}"
      },
      "package": "com.asenousy.thinksalary"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
